		<link rel="stylesheet" type="text/css" href="/static/css/css3-checkboxes-and-radios.css"/>
		<link rel="stylesheet" type="text/css" href="/static/css/customer/style.css"/>

		<script>$(document).ready(function(){ $("#[% selected_mnu %]").addClass("selected "); $("#[% selected_mnu %]").removeAttr('href'); });</script>
